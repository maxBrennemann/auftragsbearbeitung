{
    "require": {
        "tecnickcom/tcpdf": "^6.10.0",
        "prestashop/prestashop-webservice-lib": "dev-master",
        "guzzlehttp/guzzle": "^7.9.3",
        "google/apiclient": "^2.12.1",
        "symfony/dotenv": "7.3.2",
        "max/php-utilities": "1.3.1",
        "ramsey/uuid": "5.x-dev",
        "symfony/console": "7.3.4"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.4"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "phpstan": "php -d memory_limit=1G vendor/bin/phpstan analyse -c .config/phpstan.neon",
        "format": "php-cs-fixer fix --config=.config/formatter/.php-cs-fixer.dist.php"
    },
    "extra": {
        "google/apiclient-services": [
            "SearchConsole"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "phpstan/phpstan": "2.1.x-dev",
        "ergebnis/php-cs-fixer-config": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "name": "max/php-utilities",
            "url": "https://github.com/maxBrennemann/php-utilities.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Migrations\\": "database/Migrations/",
            "Src\\": "src/"
        }
    }
}
