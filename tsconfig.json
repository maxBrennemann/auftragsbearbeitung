{
  "compilerOptions": {
    "types": ["node"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {}
  },
  "include": [
    "public/res/js"
  ],
  "exclude": [
    "node_modules",
  ],
  "typeRoots": ["./node_modules/@types", "./config/types"]
}